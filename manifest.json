{
    "name": "Assist.ai - AI Assistant",
    "description": "Assist AI helps you in your day to day life tasks by assisting you in a variety of tasks over the internet.",
    "author": "Gourav Nag",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "runAt": "document_end",
            "js": ["./js/script.js"],
            "all_frames": true
        }
    ]
}